{
  "name": "asos-extension",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preview": "vite preview",
    "dev:content": "vite build --watch --config vite-content.config.js --emptyOutDir false",
    "dev:popup": "vite build --watch --config vite-popup.config.js --emptyOutDir false",
    "build": "npm run build:content && npm run build:popup",
    "build:content": "vite build --config vite-content.config.js",
    "build:popup": "vite build --config vite-popup.config.js --emptyOutDir false"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "vite": "^2.8.0"
  }
}
